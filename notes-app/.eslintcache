List.noteList.noteList.noteList}\r\n                            setNoteList={note.props.setNoteList}\r\n                            setEditorTitle={note.props.setEditorTitle}\r\n                            setEditorDate={note.props.setEditorDate}\r\n                            setEditorText={note.props.setEditorText}\r\n                        />\r\n            })\r\n            return newNoteList;\r\n        } else {\r\n            return null;\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div id=\"library-notes\">\r\n            <NoteListComp noteList={noteList} />\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default NoteList;",{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":1,"column":8,"nodeType":"60","messageId":"61","endLine":1,"endColumn":13},{"ruleId":"62","severity":1,"message":"63","line":19,"column":47,"nodeType":"64","messageId":"65","endLine":19,"endColumn":49},{"ruleId":"62","severity":1,"message":"63","line":63,"column":42,"nodeType":"64","messageId":"65","endLine":63,"endColumn":44},{"ruleId":"58","severity":1,"message":"66","line":1,"column":10,"nodeType":"60","messageId":"61","endLine":1,"endColumn":18},{"ruleId":"58","severity":1,"message":"67","line":1,"column":20,"nodeType":"60","messageId":"61","endLine":1,"endColumn":29},{"ruleId":"62","severity":1,"message":"63","line":33,"column":43,"nodeType":"64","messageId":"65","endLine":33,"endColumn":45},{"ruleId":"62","severity":1,"message":"63","line":53,"column":30,"nodeType":"64","messageId":"65","endLine":53,"endColumn":32},{"ruleId":"68","severity":1,"message":"69","line":70,"column":103,"nodeType":"70","messageId":"71","endLine":70,"endColumn":105},{"ruleId":"62","severity":1,"message":"63","line":71,"column":37,"nodeType":"64","messageId":"65","endLine":71,"endColumn":39},{"ruleId":"62","severity":1,"message":"72","line":15,"column":38,"nodeType":"64","messageId":"65","endLine":15,"endColumn":40},{"ruleId":"62","seve